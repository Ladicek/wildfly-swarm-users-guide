{
  "gitbook": "2.x.x",
  "structure": {
      "readme": "README.adoc"
  },
  "plugins": [
      "ungrey",
      "splitter",
      "expandable-chapters",
      "edit-link",
      "addcssjs"      
  ],
  "pluginsConfig": {
           "edit-link": {
               "base": "https://github.com/wildfly-swarm/wildfly-swarm-users-guide/tree/master/",
               "label": "Edit This Page"
           },
           "addcssjs": {
              "css": ["/styles/asciidoc.css"]
          }
   },
  "variables": {
      "javadoc_url_preffix" : "http://wildfly-swarm.github.io/wildfly-swarm-javadocs/",
      "versions": {
          "swarm": "2016.7-SNAPSHOT",
          "configuration": "0.4.4",
          "keycloak": "1.9.8.Final"
      }
  }
}
